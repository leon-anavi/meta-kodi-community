 src/defaults.py | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/src/defaults.py b/src/defaults.py
index f302540..8f458b9 100644
--- a/src/defaults.py
+++ b/src/defaults.py
@@ -93,7 +93,7 @@ xdbus = {'ENABLED': True}
 _services = {
     'sshd': ['sshd.service'],
     'avahi': ['avahi-daemon.service'],
-    'samba': ['nmbd.service', 'smbd.service'],
+    'samba': ['nmb.service', 'smb.service'],
     'bluez': ['bluetooth.service'],
     'obexd': ['obex.service'],
     'crond': ['cron.service'],
